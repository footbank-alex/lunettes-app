{
  "name": "lunettes-app",
  "version": "1.0.0",
  "private": true,
  "description": "Lunettes App",
  "author": "Alexander Bittermann <alexander.bittermann@footbanksystems.com>",
  "license": "MIT",
  "keywords": [
    "gatsby"
  ],
  "browserslist": [
    "defaults"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "typecheck": "tsc --noEmit",
    "develop:dev": "amplify env checkout dev && yarn run develop",
    "develop:prod": "amplify env checkout prod &&  yarn run develop",
    "develop:mock": "cross-env GATSBY_MOCK=true yarn run develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start:dev": "yarn run develop:dev",
    "start:prod": "yarn run develop:prod",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "amplify:lunettesRegister": "yarn --cwd amplify/backend/lib/lunettes-register run build",
    "amplify:lunettesSeminars": "yarn --cwd amplify/backend/lib/lunettes-seminars run build",
    "amplify:lunettesCleanup": "yarn --cwd amplify/backend/lib/lunettes-cleanup run build",
    "amplify:lunettesMigration": "yarn --cwd amplify/backend/lib/lunettes-migration run build"
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^2.19.0",
    "autoprefixer": "^10.4.7",
    "aws-amplify": "^4.3.24",
    "cssnano": "^5.1.11",
    "flagpack": "^1.0.5",
    "gatsby": "^4.15.1",
    "gatsby-plugin-image": "^2.15.0",
    "gatsby-plugin-layout": "^3.15.0",
    "gatsby-plugin-manifest": "^4.15.0",
    "gatsby-plugin-offline": "^5.12.1",
    "gatsby-plugin-portal": "^1.0.7",
    "gatsby-plugin-postcss": "^5.15.0",
    "gatsby-plugin-react-helmet": "^5.15.0",
    "gatsby-plugin-react-i18next": "^1.2.3",
    "gatsby-plugin-sharp": "^4.15.0",
    "gatsby-source-filesystem": "^4.15.0",
    "gatsby-source-graphql": "^4.15.0",
    "gatsby-source-package": "^1.0.1",
    "gatsby-transformer-sharp": "^4.15.0",
    "i18next": "^21.8.8",
    "luxon": "^2.4.0",
    "normalize.css": "^8.0.1",
    "postcss": "^8.4.14",
    "postcss-import": "^14.1.0",
    "postcss-mixins": "^9.0.2",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "^7.7.1",
    "postcss-simple-vars": "^6.0.3",
    "prop-types": "^15.8.1",
    "react": "^18.1.0",
    "react-datetime-picker": "^3.5.0",
    "react-dom": "^18.1.0",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.17.0",
    "react-icons": "^4.4.0",
    "react-portal": "^4.2.2"
  },
  "devDependencies": {
    "@types/luxon": "^2.3.2",
    "@types/node": "^17.0.36",
    "@types/react": "^18.0.9",
    "@types/react-datetime-picker": "^3.4.1",
    "@types/react-dom": "^18.0.5",
    "@types/react-helmet": "^6.1.5",
    "@types/react-portal": "^4.0.4",
    "axios-mock-adapter": "^1.21.1",
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "typescript": "^4.7.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/footbank-alex/lunettes-app"
  }
}
