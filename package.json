  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "develop:dev": "amplify env checkout dev && yarn run develop",
    "develop:prod": "amplify env checkout prod &&  yarn run develop",
    "develop:mock": "cross-env GATSBY_MOCK=true yarn run develop",
    "clean": "gatsby clean",
    "serve": "gatsby serve",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start:dev": "yarn run develop:dev",
    "start:prod": "yarn run develop:prod",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "amplify:lunettesRegister": "yarn --cwd amplify/backend/lib/lunettes-register run build",
    "amplify:lunettesEndpoints": "yarn --cwd amplify/backend/lib/lunettes-endpoints run build",
    "amplify:lunettesCleanup": "yarn --cwd amplify/backend/lib/lunettes-cleanup run build"
  },
  "devDependencies": {
    "@types/luxon": "^2.3.1",
