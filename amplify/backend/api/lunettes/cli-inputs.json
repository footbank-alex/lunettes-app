{
  "version": 1,
  "paths": {
    "/register": {
      "name": "/register",
      "lambdaFunction": "lunettesRegister",
      "permissions": {
        "setting": "open"
      }
    },
    "/seminars/{phoneNumber}": {
      "name": "/seminars/{phoneNumber}",
      "lambdaFunction": "lunettesEndpoints",
      "permissions": {
        "setting": "private",
        "auth": [
          "read"
        ]
      }
    },
    "/seminar/{endpointId}/{seminarId}": {
      "name": "/seminar/{endpointId}/{seminarId}",
      "lambdaFunction": "lunettesEndpoints",
      "permissions": {
        "setting": "private",
        "auth": [
          "update",
          "delete"
        ]
      }
    }
  }
}